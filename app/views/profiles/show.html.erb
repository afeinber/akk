    <ul class="tabs" data-tab>
      <li class="tab-title active profile-tab-wrapper"><a href="#profile">Profie</a></li>
      <li class="tab-title profile-tab-wrapper"><a href="#my-images">My images</a></li>
      <li class="tab-title profile-tab-wrapper"><a href="#followings">Followings</a></li>
      <li class="tab-title profile-tab-wrapper"><a href="#followers">Followers</a></li>
    </ul>

    <div class="tabs-content">
      <div class="content active" id="profile">
        <p>Username: <%= @profile.username %></p>
        <p>Blurb: <%= @profile.blurb %></p>
        <p>Avatar: <%= image_tag @profile.avatar.url(:thumb) %></p>
      </div>
      <div class="content" id="my-images">
        <% @profile.images.each do |image|%>
          <%= image_tag image.sharedimg.url(:thumb) %>
        <%end%>
      </div>
      <div class="content" id="followings">
        <% @profile.follows.each do |follow|%>
          <%= link_to follow.followee.username, profile_path(follow.followee.id) %>
        <%end%>
      </div>
      <div class="content" id="followers">
        <% @profile.inverse_follows.each do |i_follow|%>
          <%= link_to i_follow.user.username, profile_path(i_follow.user.id) %>
        <%end%>
      </div>
    </div>
